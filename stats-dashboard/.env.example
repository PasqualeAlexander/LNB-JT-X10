# Credenciales de la base de datos para MySQL
# Copia este archivo a .env y rellena con tus datos reales.

# Host de la base de datos (ej. localhost o una IP)
DB_HOST=localhost

# Puerto de la base de datos (normalmente 3306 para MySQL)
DB_PORT=3306

# Nombre de la base de datos
DB_NAME=lnb_estadisticas

# Usuario de la base de datos
DB_USER=lnb_user

# Contrase√±a del usuario de la base de datos
DB_PASSWORD=lnb_password
